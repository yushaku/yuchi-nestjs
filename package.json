{
  "name": "yuchi-api",
  "version": "0.0.1",
  "author": "yushaku",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "dev": "nest start -b swc --watch",
    "start:debug": "nest start -b swc --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:up": "pnpm prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit --detectOpenHandles",
    "console": "pnpm build && LOG_QUERY=0 node dist/console"
  },
  "dependencies": {
    "@fastify/cookie": "^9.1.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.2.7",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/throttler": "^6.1.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "prisma": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.2",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "nestjs-throttler-storage-redis": "^0.5.0",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "google-auth-library": "^9.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@swc/jest": "^0.2.29",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
